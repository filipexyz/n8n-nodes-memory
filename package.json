  "devDependencies": {
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^2.2.1"
  },
  "dependencies": {
    "@langchain/core": "^0.3.0",
    "langchain": "^0.3.0"
  }
